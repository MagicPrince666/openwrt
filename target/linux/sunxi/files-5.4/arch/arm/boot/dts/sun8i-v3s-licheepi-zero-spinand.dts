&spi0 {
	//#address-cells = <1>;
	//#size-cells = <0>;
	pinctrl-names = "default";
	pinctrl-0 = <&spi0_pins &spi0_pins_cs1>;
	spi-num-chipselects = <2>;
	cs-gpios = <&pio 2 2 GPIO_ACTIVE_HIGH>, <&pio 6 3 GPIO_ACTIVE_HIGH>;
	status = "okay";

	flash: spinand@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		compatible = "spi-nand";
		spi-max-frequency = <50000000>;
		status = "okay";

		boot@0 {
			label = "boot";
			reg = <0x0000000 0x0100000>;
		};

		ubi0@100000 {
			label = "ubi0";
			reg = <0x0100000 0x0000000>;
		};
	};

	spidev: spidev@1 {
		reg = <1>;
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <40000000>;
		status = "okay";
	};
};

&pio {

	key_pins: key_pins@0 {
		pins = "PB0", "PB1";
		function = "gpio_in";
		bias-pull-up;
	};
/*
	i2c0_pins: i2c0 {
		pins = "PB6", "PB7";
		function = "i2c0";
	};
*/
	spi0_pins: spi0-pins {
		pins = "PC0", "PC1", "PC2", "PC3";
		function = "spi0";
	};
	
	spi0_pins_cs1: spi0_pins_cs1 {
		pins = "PC2", "PG3";
		function = "gpio_out";
	};

	twi1_clk: i2c1 {
		pins = "PB8";
		function = "i2c1";
	};

	bbpwm_pins: pppwm { /* PWM for BL as BITBANG-PWM */
		pins = "PB9";
		function = "gpio_out";
	};

	pwm1_pins: pwm1_pins {
		pins = "PB5";
		function = "pwm";
	};
	
	/* Parallel CSI related */
	csi1_8bit: csi1-8bit@0 {
		pins = "PE6","PE7","PE8","PE9","PE10","PE11","PE12","PE13","PE14","PE15";
		bias-disable;
		function = "csi";
	};

	csi1_clk: csi1-clk@0 {
		pins = "PE0","PE2","PE3";
		bias-disable;
		function = "csi";
	};

	csi1_mclk: csi1-mclk@0 {
		pins = "PE1";
		bias-disable;
		function = "csi";
	};

	uart0_pins: uart0@0 {
		pins = "PB8", "PB9";
		function = "uart0";
	};

	uart1_pins: uart1@0 {
		pins = "PE21", "PE22";
		function = "uart1";
		bias-pull-up;
	};

	uart2_pins: uart2@0 {
		pins = "PB0", "PB1";
		function = "uart2";
		bias-pull-up;
	};

	lcd_rgb666_pins_a: lcd-rgb666-pe {
		pins = "PE0", "PE1", "PE2", "PE3", "PE4", "PE5",
				"PE6", "PE7", "PE8", "PE9", "PE10", "PE11",
				"PE12", "PE13", "PE14", "PE15", "PE16", "PE17",
				"PE18", "PE19", "PE23", "PE24";
		function = "lcd";
	};

	mmc0_pins_a: mmc0@0 {
		pins = "PF0", "PF1", "PF2", "PF3",
				"PF4", "PF5";
		function = "mmc0";
		drive-strength = <30>;
		bias-pull-up;
	};
/*
	mmc1_pins_a: mmc1 {
